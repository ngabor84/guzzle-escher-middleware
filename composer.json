{
    "name": "ngabor84/guzzle-escher-middleware",
    "description": "Escher authentication middleware for Guzzle Http Client",
    "keywords": ["guzzle", "http", "middleware", "escher"],
    "homepage": "https://github.com/ngabor84/guzzle-escher-middleware",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabor Nemeth",
            "email": "negabor@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/http-message": "1.0.1",
        "guzzlehttp/guzzle": ">=6.3",
        "emartech/escher": ">=1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.1.3",
        "squizlabs/php_codesniffer": ">=3.2.3"
    },
    "scripts": {
        "check": "./vendor/bin/phpcs --colors",
        "fix": "./vendor/bin/phpcbf",
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "autoload": {
        "psr-4": {
            "Guzzle\\Http\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guzzle\\Http\\Middleware\\Tests\\": "test"
        }
    }
}
